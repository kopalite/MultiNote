<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<!-- Identifier that must be unique within the hosting gallery -->
		<id>MultiNote</id>

		<!-- Package version number that is used when resolving dependencies -->
		<version>1.1.5</version>

		<!-- Authors contain text that appears directly on the gallery -->
		<authors>Ivan Kopcanski</authors>

		<!-- 
            Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  
        -->
		<owners>kopalite</owners>

		<!-- Project URL provides a link for the gallery -->
		<projectUrl>https://github.com/kopalite/MultiNote</projectUrl>

		<!-- License information is displayed on the gallery -->
		<license type="expression">MIT</license>

		<!-- 
            If true, this value prompts the user to accept the license when
            installing the package. 
        -->
		<requireLicenseAcceptance>false</requireLicenseAcceptance>

		<!-- Any details about this particular release -->
		<releaseNotes>Initial version</releaseNotes>

		<!-- 
            The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. 
        -->
		<description>MultiNote is notification library utilizing multiple messaging channels like email, Slack webhooks etc. For details, please refer the project README file on GitHub.</description>

		<!-- Copyright information -->
		<copyright>Copyright ©2021 kopalite</copyright>

		<!-- Tags appear in the gallery and can be used for tag searches -->
		<tags>notifications email slack dependencyinjection</tags>

		<!-- Dependencies are automatically installed when the package is installed -->
		<dependencies>
			<group targetFramework="net5.0">
				    <dependency id="Microsoft.Extensions.Configuration.Abstractions" version="5.0.0" />
					<dependency id="Microsoft.Extensions.Logging.Abstractions" version="5.0.0" />
					<dependency id="Microsoft.Extensions.Options" version="5.0.0" />
					<dependency id="Microsoft.Extensions.Options.ConfigurationExtensions" version="5.0.0" />
					<dependency id="Slack.Webhooks" version="1.1.4" />
			</group>
		</dependencies>
	</metadata>

	<files>
		<file src="MultiNote.dll" target="lib\net5.0" />
	</files>
	
	
</package>